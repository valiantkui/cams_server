<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<!-- namespace映射文件的namespace必须等于Mapper映射器的全限定名 -->
<mapper namespace="kui.cams.dao.R_c_sDao">
	<!-- resource(r_no, type, subject, path, md5, count)  -->
	<!-- r_c_s(r_no, c_no, r_name, s_id)  -->
	
	<insert id="addR_c_s"  parameterType="kui.cams.entity.R_c_s">
		insert into r_c_s(r_no,c_no,r_name,upload_date,s_id)
		values(#{r_no},#{c_no},#{r_name},#{upload_date},#{s_id})
	</insert>
	
	

	
	
	
</mapper>







